<?xml version="1.0" encoding="UTF-8"?>
<suite  name="Nop commerce - User">

	<listeners>
		<listener class-name="commons.MethodListener"></listener>
		<listener class-name="reportConfig.ReportNGListener"></listener>
		<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
		<listener class-name="reportConfig.ExtentReportListenerV2"></listener>
	</listeners>
	<parameter name="url" value="https://demo.nopcommerce.com/" />
	<test name="Run on Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.nopcommerce.user.Level_15_AttachScreenShotToReportNG" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->